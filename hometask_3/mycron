* * * * 1-5 root date >> /var/log/logged-in.log && echo "SYSTEM UPTIME:  " >> /var/log/logged-in.log && w >> /var/log/logged-in.log && echo "MEMORY USAGE:  " >> /var/log/logged-in.log && free -m >> /var/log/logged-in.log && echo "SPACE USAGE:  " >> /var/log/logged-in.log && df -h >> /var/log/logged-in.log && echo "LIST OF PROGRAMS USED OPEN TCP PORTS:  " >> /var/log/logged-in.log && ss -tulpn | grep '^tcp' >> /var/log/logged-in.log && echo "CONNECTION TO ukr.net:  " >> /var/log/logged-in.log && ping -c2 ukr.net >> /var/log/logged-in.log &&echo "LIST OF SUID PROGRAMS:  " >> /var/log/logged-in.log && find / -perm -4000 >> /var/log/logged-in.log && echo "<<<--------------------------------------->>>" >> /var/log/logged-in.log